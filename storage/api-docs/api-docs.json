                "operationId": "3278597f70ffcf3df60a694e0ff0d1c1",
                "responses": {
                    "200": {
                        "description": "List of notebook entries",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Notebook"
                                    }
                                }
                            }
                        }
                    }
                }
            },
